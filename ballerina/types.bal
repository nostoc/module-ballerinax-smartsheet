# Representation of a product configuration overview, a condensed read-only view of a product configuration. Contains only the selected values
public type CCPConfigurationOverview record {
    # Total number of incomplete attributes
    int:Signed32 numberOfIncompleteCharacteristics?;
    # Product code of configuration's root product
    string productCode?;
    # For filtering the configuration overview according to the UI groups
    CCPFilterData[] groupFilterList?;
    # Total number of conflicts
    int:Signed32 numberOfConflicts?;
    # Configuration overview groups
    CCPGroupOverview[] groups?;
    # Message list
    CCPMessage[] messages?;
    # Configuration Identifier. A randomly generated UUID owned by the product configurator
    string id?;
    # Pricing is enabled, meaning that pricing related APIs are called and prices are shown during configuration
    boolean pricingEnabled?;
    # Identifier of document that owns this configuration. Can be the identifier of a saved cart, a quote, or an order
    string sourceDocumentId?;
    # For filtering the configuration overview according to attribute facets such as price relevance or attribute author
    CCPFilterData[] appliedCsticFilter?;
    # Total number of issues: sum of number of conflicts and number of incomplete mandatory fields
    int:Signed32 totalNumberOfIssues?;
    # Prices on overall product configuration level
    CCPConfigurationOverallPricing pricing?;
};


# Representation of a order. Note that sapBillingAddress is mutually exclusive with paymentInfo.billingAddress, which is used when the billing address is created concurrently with the paymentInfo. sapBillingAddress is used when there is a need to save the billing address, but the related paymentInfo has not been created yet
public type Order record {
    # User's object
    User orgCustomer?;
    # List of applied product promotions
    PromotionResult[] appliedProductPromotions?;
    # Email of customer
    string sapCustomerEmail?;
    # The payment option identifier. It specifies which option the consumer selected to make the payment for the cart
    string sapPaymentOptionId?;
    # Representation of a Price
    Price subTotal?;
    # Boolean flag showing if order is cancellable
    boolean cancellable?;
    # Representation of a Price
    Price totalTax?;
    # List of applied vouchers
    Voucher[] appliedVouchers?;
    # Representation of a Delivery mode
    DeliveryMode deliveryMode?;
    # Address object
    Address deliveryAddress?;
    # Flag stating iv value is net-value
    boolean net?;
    # Flag showing if order is calculated
    boolean calculated?;
    # Address object
    Address sapBillingAddress?;
    # Representation of a Price
    Price productDiscounts?;
    # List of applied order promotions
    PromotionResult[] appliedOrderPromotions?;
    # Date of order creation
    string created?;
    # Cost center object
    B2BCostCenter costCenter?;
    # Organizational unit object
    B2BUnit orgUnit?;
    # Chinese payment info
    ChinesePaymentInfo chinesePaymentInfo?;
    # Representation of a Price
    Price orderDiscounts?;
    # Address object
    Address paymentAddress?;
    # List of order entries
    OrderEntry[] entries?;
    # Boolean flag showing if order is returnable
    boolean returnable?;
    # List of unconsigned order entries
    OrderEntry[] unconsignedEntries?;
    # Total number of individual units across all items in the order
    int:Signed32 totalUnitCount?;
    # Guest user id identifier
    string guid?;
    # Order delivery status display
    string deliveryStatusDisplay?;
    # List of delivery order entries group
    DeliveryOrderEntryGroup[] deliveryOrderGroups?;
    # Order delivery status
    string deliveryStatus?;
    # Status of order
    string status?;
    # Customer requested date for order retrieval
    string requestedRetrievalAt?;
    # Code number of order
    string code?;
    # Representation of a Price
    Price totalPrice?;
    # Representation of a Price
    Price totalDiscounts?;
    # Flag showing if customer is Guest customer
    boolean guestCustomer?;
    # If Service product Exist,Service Order is Reschedubale or not.If Service Product Doesnt Exits the valuue will be set as False. The flags are used to show/hide the 'Service Order Reschedule' button in UI
    boolean serviceReschedulable?;
    # Payment type configuration for B2B transactions
    B2BPaymentType paymentType?;
    # Requested Seervice Date for all service products in cart/Order
    string servicedAt?;
    # data type of tax invoice
    TaxInvoice taxInvoice?;
    # Quantity of delivery items
    int deliveryItemsQuantity?;
    # Representation of a Price
    Price totalPriceWithTax?;
    # Quote Code associated with the Order. The Quote Code is present if the order was placed from a quote
    string sapQuoteCode?;
    # Payment status
    string paymentStatus?;
    # Results of permissions associated with the order
    OrderApprovalPermissionResult[] permissionResults?;
    # If Service product Exist,based on condition Service Order is cancellable or not.If Service Product Doesnt Exits the valuue will be set as False.The flags are used to show/hide the 'Service Order Cancel' button in UI
    boolean serviceCancellable?;
    # List of entry groups
    EntryGroup[] entryGroups?;
    # Total number of distinct items in the order
    int:Signed32 totalItems?;
    # Status display
    string statusDisplay?;
    # List of pickup order entry group
    PickupOrderEntryGroup[] pickupOrderGroups?;
    # Store
    string store?;
    # Site
    string site?;
    # data type of delivery timeslot
    DeliveryTimeSlot deliveryTimeSlot?;
    # Purchase order number
    string purchaseOrderNumber?;
    # Billing documents associated with the order
    BillingDocument[] billingDocuments?;
    # Quantity of pickup items
    int pickupItemsQuantity?;
    # List of consignment
    Consignment[] consignments?;
    # Representation of a Principal webservice DTO used for defining User data types
    Principal user?;
    # Payment details object
    PaymentDetails paymentInfo?;
    # Representation of a Price
    Price deliveryCost?;
};

